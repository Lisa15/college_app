<h1>Listing all Events</h1>
<p>
<%= link_to "Create new Event", new_event_path %>
</p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

  <% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.description %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Show', event_path(event) %></td>
  </tr>
  <% end %>
</table>